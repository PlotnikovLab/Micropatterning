SelectOptConf("Print_Fudiciary_Marker"); 
ND_RunZSeriesExp(); //burn a small square through many Z-planes
SelectOptConf("Autofocus"); 
StgFocus(); //Use autofocus to find the best focal plane
SelectOptConf("Load_ROI"); 
CloseAllDocuments(2);
Capture(); //capture a 2048 image to load the ROI which is also 2048
ClearMeasROI();
LoadROI("E:\Chen\Patterning\Pattern\2048 200px T.tif",1); //load ROI from directory
ChangeROIType(1,3); //change the individual ROIs in the set of ROIs from regular ROI to stimulation ROI
ChangeROIType(2,3);
ChangeROIType(3,3);
ChangeROIType(8,3);
ChangeROIType(9,3);
ChangeROIType(12,3);
ChangeROIType(13,3);
ChangeROIType(14,3);
A1ApplyStimulationSettings(); //Apply Stimulation settings in ND stimulation
